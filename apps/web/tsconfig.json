{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "jsx": "preserve",
        "allowJs": true,
        "noEmit": true,
        "incremental": true,
        "isolatedModules": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "include": ["next-env.d.ts", "src/**/*.ts", "src/**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
